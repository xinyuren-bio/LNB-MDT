# Auto detect text files and perform LF normalization
* text=auto

# Explicitly declare text files you want to always be normalized and converted
# to native line endings on checkout.
*.py text eol=lf
*.pyw text eol=lf
*.sh text eol=lf
*.bat text eol=crlf
*.md text eol=lf
*.txt text eol=lf
*.rst text eol=lf
*.yaml text eol=lf
*.yml text eol=lf
*.json text eol=lf
*.ini text eol=lf
*.cfg text eol=lf
*.conf text eol=lf

# UI files
*.ui text eol=lf
*.qrc text eol=lf
*.qss text eol=lf

# Script files
*.tcl text eol=lf

# Denote all files that are truly binary and should not be modified.
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.pdf binary
*.zip binary
*.tar.gz binary
*.whl binary
*.gro binary
*.xtc binary
*.trr binary
*.tpr binary
*.ndx binary
*.itp binary
*.mdp binary
